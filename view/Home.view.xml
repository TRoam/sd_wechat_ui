<mvc:View
   controllerName="sap.ui.sd.controller.Home"
   xmlns="sap.m"
   xmlns:mvc="sap.ui.core.mvc">
    <Page title="{i18n>homePageTitle}">
    <headerContent>
            <Button
                icon="sap-icon://hello-world"
                press="onOpenDialog"/>
    </headerContent>
    <content>
        <Button text="{i18n>iWantToNavigate}" class="sapUiTinyMarginEnd"/>
        <mvc:XMLView viewName="sap.ui.sd.view.HelloPanel"/>
        <mvc:XMLView viewName="sap.ui.sd.view.InvoiceList"/>
    </content>
    </Page>
</mvc:View>